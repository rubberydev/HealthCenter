<?xml version="1.0" encoding="utf-8" ?>
<Application xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
              xmlns:infra="clr-namespace:HealthCenter.Infrastructure"
             x:Class="HealthCenter.App">
	<Application.Resources>
        <ResourceDictionary>
            <infra:InstanceLocator x:Key="Locator"/>
            <x:String x:Key="APISecurity">http://healthcenterapi.azurewebsites.net</x:String>

            <!--Key autentication Instagram-->
            <x:String x:Key="InstagramAppID">5c7d845b21224579a1f63301fa0feb22</x:String>
            <x:String x:Key="InstagramAuthURL">https://api.instagram.com/oauth/authorize/</x:String>
            <x:String x:Key="InstagramRedirectURL">https://www.instagram.com/</x:String>
            <x:String x:Key="InstagramScope">basic</x:String>
            <x:String x:Key="InstagramProfileInfoURL">
                https://api.instagram.com/v1/users/self/?access_token
            </x:String>
        </ResourceDictionary>
    </Application.Resources>
</Application>