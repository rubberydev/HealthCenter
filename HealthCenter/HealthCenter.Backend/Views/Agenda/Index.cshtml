        <td>
            @Html.DisplayFor(modelItem => item.WorkDay.DateToday)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Medics.Email)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.startHour)
        </td>
