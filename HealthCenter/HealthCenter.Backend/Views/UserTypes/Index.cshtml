@model IEnumerable<HealthCenter.Domain.UserType>

@{
    ViewBag.Title = "Patient type";
}

<h2>Patient type</h2>

<p>
    @Html.ActionLink("Create New patient type", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.UserTypeId }) |
            @Html.ActionLink("Details", "Details", new { id=item.UserTypeId }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.UserTypeId })
        </td>
    </tr>
}

</table>
